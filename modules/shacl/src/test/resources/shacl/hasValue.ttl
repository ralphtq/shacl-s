prefix : <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:S	a sh:NodeShape ;
	sh:property [
	 sh:path :p;
	 sh:hasValue 1 
	] .
:NotS a sh:NodeShape ;
      sh:not :S .
	
:good1 :p 1 .
:good2 :p 1; :q 2 .

:bad1 :p 2 .
:bad2 :q 1 .
:bad3 :p 1; :p 2 .
:bad4 :p 2; :q 2 .


:S sh:targetNode 
  :good1 ,
  :good2 .
:NotS sh:targetNode 
  :bad1, 
  :bad2, 
  :bad3, 
  :bad4 .


