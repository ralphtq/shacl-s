prefix : <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#>

:S a sh:NodeShape;
   sh:property :hasP .

:hasP sh:path :p;
     sh:minCount 1 ;
     sh:maxCount 2 ;
    .

:x :p "a" .

:y :p 1, 2 .

:NotS a sh:NodeShape ;
  sh:not :S .

#:S sh:targetNode :x, :y .
:NotS sh:targetNode :z .

:z :q 1 .
