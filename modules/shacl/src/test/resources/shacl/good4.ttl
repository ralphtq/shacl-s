prefix : <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

:S a sh:NodeShape; 
   sh:targetNode :x, :y, :z, <t> ;
   sh:property [
     sh:path :p; 
     sh:datatype xsd:string
   ];
   sh:property [
     sh:path :q; 
     sh:datatype xsd:integer
   ] ;
   sh:nodeKind sh:IRI .

:x :p "Hi"; 
   :q 23.

:y :p "Hi", "Bye" .

:z :q "23"^^xsd:integer, 34 .

<t> :p "Hi" .
