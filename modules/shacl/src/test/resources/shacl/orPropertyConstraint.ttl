prefix : <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:S a sh:NodeShape;
   sh:property [
    sh:path :p ;
    sh:or (
      [ sh:class :t0 ]
      [ sh:class :t1 ]
    )
   ]
.

:S sh:targetNode :x0, :x1, :x01 .
:NotS sh:targetNode :x2 .
:NotS sh:not :S .


:x0 :p [ a :t0 ] .
:x1 :p [ a :t1 ] .
:x01 :p [a :t0 ], [a :t1 ] .
:x2 :p [ a :t2 ] .
