prefix : <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#>

:S a sh:NodeShape; 
   sh:targetNode :x, :y, :z ;
   sh:property [
     sh:path :p; 
     sh:nodeKind sh:IRI
   ];
   sh:property [
     sh:path :q; 
     sh:nodeKind sh:Literal
   ] .

:x :p :x ;
   :q 23 .

:y :q 1, 2 .

:z :p :y .

:NotS a sh:NodeShape ;
  sh:not :S .
  
:NotS sh:targetNode :bad1 .

:bad1 :p [] .
